package family;

abstract class TextDemo {
   abstract  void chinni(int a,int b);
}
public class TextDemo1 extends TextDemo{
      void chinni(int a,int b)
      {
          int c=a+b;
    	  System.out.println(" addition is:" + c );
      }
}
public class TextDemoMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        TextDemo1 d1=new TextDemo1();
        d1.chinni(100,300);
	}

}
output: addition is: 400
