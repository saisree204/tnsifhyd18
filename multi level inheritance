package com.si.sample;

public class Parent2 {
      public void maa()
      {
    	 System.out.println("mom is eating"); 
      }
}
public class Child2 extends Parent1
{
     public void chi()
     {
    	 System.out.println("child2 is eating");
     }
}
public class Child3 extends Child2
{
    public void bro()
    {
    	System.out.println("child3 is eating");
    }
}
public class Parent2Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       
       Child3 c3=new Child3();
       c3.chi();
       c3.bro();
       c3.maa();
	}

}
